generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CommitAnalysis {
  id            String   @id @default(uuid())
  repository    String
  branch        String
  commitHash    String
  commitMessage String
  profanity     String[]
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
} 