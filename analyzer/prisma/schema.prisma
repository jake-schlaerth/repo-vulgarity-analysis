generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model commit_analysis {
  id             String   @id
  repository     String
  branch         String
  commit_hash    String
  commit_message String
  profanity      String[]
  created_at     DateTime @default(now())
  updated_at     DateTime
}
